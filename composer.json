{
    "name": "Noman73/number-to-bangla",
    "description": "Laravel package to convert English numbers to Bangla number or Bangla text, Bangla month name and Bangla Money Format",
    "keywords": ["laravel","Laravel-number-bangla","number-bangla","number-month"],
    "homepage": "https://github.com/Noman73/number-to-bangla",
    "license": "MIT",
    "authors": [
        {
            "name": "Abdulla-al-Noman",
            "email": "noman.eng73@gmail.com",
            "homepage": "https://github.com/Noman73/number-to-bangla"
        }
    ],
    "require": {
        "php": "^7.1|^7.2|^7.3|^7.4|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Rakibhstu\\Banglanumber\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rakibhstu\\Banglanumber\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rakibhstu\\Banglanumber\\NumberToBanglaServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
